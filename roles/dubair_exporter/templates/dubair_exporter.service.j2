# {{ ansible_managed }}
[Unit]
Description=Prometheus dubair exporter
Documentation=https://github.com/relvira/dubair_exporter
Requires=dubair_exporter.service
Wants=network-online.target
After=network-online.target dubair_exporter.service

[Service]
User=dubair_exporter
Group=dubair_exporter
Type=simple
ExecStart={{ host_bin_path }}/dubair_exporter

[Install]
WantedBy=multi-user.target
