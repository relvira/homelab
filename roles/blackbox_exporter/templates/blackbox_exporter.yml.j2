---
# {{ ansible_managed }}
modules:
  http_2xx:
    prober: http
    http:
      preferred_ip_protocol: ip4
  http_401:
    prober: http
    http:
      preferred_ip_protocol: ip4
      valid_status_codes:
        - 401
  http_post_2xx:
    prober: http
    http:
      preferred_ip_protocol: ip4
      method: POST
  tcp_connect:
    prober: tcp
    timeout: 5s
  ssh_banner:
    prober: tcp
    tcp:
      query_response:
      - expect: "^SSH-2.0-"
  icmp:
    prober: icmp
